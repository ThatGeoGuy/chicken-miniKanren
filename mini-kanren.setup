;; -*- Hen -*- vim: ft=scheme

(define (dynld-name fn)
  (make-pathname #f fn ##sys#load-dynamic-extension))

(compile -O3 -d2 -s mini-kanren.scm -j mini-kanren)
(compile -O3 -d2 -s mini-kanren.import.scm)

(install-extension
  'mini-kanren
  `(,(dynld-name "mini-kanren") ,(dynld-name "mini-kanren.import"))
  `((version "1.3.1")))
